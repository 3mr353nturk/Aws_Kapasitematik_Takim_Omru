<style>
    .tooltip.top .tooltip-inner {
        max-width: 310px;
        padding: 3px 8px;
        color: #fff;
        text-align: center;
        background-color: #245973;
        border-radius: 5px
    }

    .editor-field textarea {
        width: 400px;
        height: 110px;
    }

    .flaticon2-bell-alarm-symbol .badge {
        position: absolute;
        top: -10px;
        right: -10px;
        padding: 3px 6px;
        border-radius: 50%;
        background: red;
        color: white;
        font-size: 11px;
    }
